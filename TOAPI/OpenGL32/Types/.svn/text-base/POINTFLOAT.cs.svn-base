using System;
using System.Runtime.InteropServices;

namespace TOAPI.OpenGL
{
    [Serializable]
    [StructLayout(LayoutKind.Sequential)]
    public struct POINTFLOAT
    {
        public float x;
        public float y;

        public POINTFLOAT(POINTFLOAT aPoint)
        {
            x = aPoint.x;
            y = aPoint.y;
        }
    }
}